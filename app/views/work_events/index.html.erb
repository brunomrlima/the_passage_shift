<% provide(:title, "Work Events") %>

<div class="container">
  <%= month_calendar events: @work_events do |date, meetings| %>
    <%= date.day %>

    <% meetings.each do |meeting| %>
      <div>
        <%= meeting.name %>
      </div>
    <% end %>
  <% end %>
</div>

<% if current_user.admin? %>
  <%= render 'new' %>
<% end %>